    "esm": "^3.2.25",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "file-stream-rotator": "^1.0.0",
    "mocha": "^10.2.0",
    "mongoose": "^7.0.3",
    "mongoose-schema-extend": "^0.2.2",
